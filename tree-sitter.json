{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "rifleconf",
      "camelcase": "RifleConf",
      "title": "rifleconf",
      "scope": "source.rifleconf",
      "file-types": [
        "conf"
      ],
      "injection-regex": "^rifleconf$",
      "class-name": "TreeSitterRifleconf"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "rifle (ranger riflemanager file opener) grammar for tree-sitter",
    "authors": [
      {
        "name": "purarue",
        "url": "https://github.com/purarue/"
      }
    ],
    "links": {
      "repository": "https://github.com/purarue/tree-sitter-rifleconf"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}